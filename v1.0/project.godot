; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Bug Touch"
run/main_scene="res://Scenes/Main_Menu.tscn"

[autoload]

Autoloader="*res://Scripts/Autoloader.gd"
AutoloadCalibrage="*res://Scripts/Autoload_calibrage.py"
Save="*res://Scripts/Save/save.gd"
Global="*res://Scripts/Global.gd"
Music="*res://Scenes/Music.tscn"
SceneHistory="*res://Scripts/Scene_history.gd"
MusicManager="*res://Scripts/MusicManager.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gdnative]

singletons=[ "res://pythonscript.gdnlib" ]

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input]

Fullscreen={
"deadzone": 0.5,
"events": [  ]
}
Windowed={
"deadzone": 0.5,
"events": [  ]
}
Back={
"deadzone": 0.5,
"events": [  ]
}
pause={
"deadzone": 0.5,
"events": [  ]
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
